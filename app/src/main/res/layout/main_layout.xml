<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="640px"
    android:layout_height="320px"
    android:keepScreenOn="true"
    android:orientation="horizontal"
    android:paddingLeft="0dp"
    android:paddingRight="0dp">

    <ImageView
        android:id="@+id/navstatus_image_view"
        android:layout_width="100px"
        android:layout_height="50px"
        android:src="@drawable/status_position_wait_wh_96_96" />

    <TextView
        android:id="@+id/distance_text_view"
        android:layout_width="120px"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/navstatus_image_view"
        android:gravity="left"
        android:text="HUHU"
        android:textSize="26px" />

    <TextView
        android:id="@+id/time_view"
        android:layout_width="450px"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="right"
        android:text="23:14"
        android:textSize="26px" />

    <TextView
        android:id="@+id/eta_text_view"
        android:layout_width="220px"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <ImageView
        android:id="@+id/gpstatus_image_view"
        android:layout_width="100px"
        android:layout_height="50px"
        android:layout_alignParentRight="true"
        android:src="@drawable/gui_strength_0_48_48" />

    <ImageView
        android:id="@+id/routespeed_image_view"
        android:layout_width="100px"
        android:layout_height="100px"
        android:layout_marginLeft="390px"
        android:layout_marginTop="207px" />

    <TextView
        android:id="@+id/nextstreetname_text_view"
        android:layout_width="640px"
        android:layout_height="wrap_content"
        android:layout_below="@+id/navstatus_image_view"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text=""
        android:textSize="40px" />

    <ImageView
        android:id="@+id/navimage_view"
        android:layout_width="100px"
        android:layout_height="110px"
        android:contentDescription="navimage"
        android:layout_marginLeft="50px"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:src="@drawable/navit_96_96" />


    <TextView
        android:id="@+id/currentspeed_text_view"
        android:layout_width="540px"
        android:layout_height="108px"
        android:layout_below="@+id/nextstreetname_text_view"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25px"
        android:gravity="center"
        android:text=""
        android:textSize="100px" />

    <ImageView
        android:id="@+id/compas_image_view"
        android:layout_width="100px"
        android:layout_height="100px"
        android:layout_marginLeft="490px"
        android:layout_centerVertical="true" />

    <TextView
        android:id="@+id/nextmanlength_text_view"
        android:layout_width="100px"
        android:layout_height="30px"
        android:layout_marginTop="30px"
        android:layout_marginLeft="50px"
        android:layout_below="@+id/currentspeed_text_view"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/currentstreetname_text_view"
        android:layout_width="640px"
        android:layout_height="33px"

        android:layout_below="@+id/lat_text_view"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/directdistance_text_view"
        android:layout_width="140px"
        android:layout_height="30px"
        android:layout_marginLeft="470px"
        android:layout_below="@+id/currentspeed_text_view"
        android:gravity="center"
        android:text="10000km"
        android:textColor="#0F0"
        android:textSize="26px" />

    <TextView
        android:id="@+id/gpsheight_text_view"
        android:layout_width="100px"
        android:layout_height="30px"
        android:layout_below="@+id/directdistance_text_view"
        android:layout_marginLeft="480px"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/lon_text_view"
        android:layout_width="150px"
        android:layout_height="30px"
        android:layout_below="@+id/currentspeed_text_view"
        android:layout_centerHorizontal="true"
        android:fontFamily="serif-monospace"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/lat_text_view"
        android:layout_width="150px"
        android:layout_height="30px"
        android:layout_below="@+id/lon_text_view"
        android:layout_centerHorizontal="true"
        android:fontFamily="serif-monospace"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/obdspeed_text_view"
        android:layout_width="200px"
        android:layout_height="30px"

        android:layout_below="@+id/currentstreetname_text_view"
        android:layout_alignParentLeft="true"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/obdcoolanttemp_text_view"
        android:layout_width="200px"
        android:layout_height="30px"
        android:layout_below="@+id/currentstreetname_text_view"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

    <TextView
        android:id="@+id/obdvoltage_text_view"
        android:layout_width="200px"
        android:layout_height="30px"
        android:layout_below="@+id/currentstreetname_text_view"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:text=""
        android:textSize="26px" />

</RelativeLayout>